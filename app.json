{
  "expo": {
    "name": "MohaTest",
    "slug": "MohaTest",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icons/app-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tuanlv.MohaTest"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tuanlv.MohaTest"
    },
    "web": {
      "favicon": "./app/assets/icons/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./app/themes/fonts"
          ]
        }
      ]
    ]
  }
}